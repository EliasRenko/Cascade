{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "haxe",
			"args": "active configuration",
			"problemMatcher": [
				"$haxe-absolute",
				"$haxe",
				"$haxe-error",
				"$haxe-trace"
			],
			"group": "build",
			"label": "haxe: active configuration"
		},
		{
			"label": "Run",
			"type": "shell",
			"command": "haxelib",
			"args": [
				"run",
				"cascade",
				"-clean",
				"all",
				"x",
				"y"
			],
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"options": {
				"cwd": "${workspaceFolder:cascade}"
			}
		},
		{
			"label": "Run project test",
			"type": "shell",
			"command": "haxelib",
			"args": [
				"run",
				"cascade",
				"-clean",
				"all"
			],
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"options": {
				"cwd": "C:/Users/Administrator/Documents/GitHub/test"
			}
		}
	]
}